<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
   <suite name="qtmozembed-tests" domain="ui">
       <description>QtMozEmbed Application automatic tests</description>
       <set name="unit-tests" feature="QtMozEmbed">
           <description>QML component unit tests</description>
           <pre_steps>
               <step>/usr/bin/start-ui-test.sh</step>
           </pre_steps>
           <post_steps>
               <step>/usr/bin/stop-ui-test.sh</step>
           </post_steps>
           <case manual="false" name="unittests-context">
               <step>cd /opt/tests/qtmozembed/auto/desktop-qt5/context &amp;&amp; ../../run-tests.sh</step>
